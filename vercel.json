{
    "name": "opti-lena",
    "headers": 
        [
            {
                "source": "/index.html",
                "headers" : [
                    {
                        "key" : "Content-Security-Policy",
                        "value" : "default-src 'self'"
                    },
                    {
                        "key" : "Cache-Control",
                        "value" : "public, max-age=0, must-revalidate"
                    },
                    {
                        "key" : "X-Frame-Options",
                        "value" : "sameorigin"
                    }
                ]
            }
        ]
}